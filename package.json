{
  "name": "ts-docker-example",
  "version": "1.0.0",
  "description": "Empty TypeScript Docker Template",
  "main": "./dist/index.js",
  "scripts": {
    "test": "npm run test:unit && npm run test:coverage",
    "start": "node .",
    "dev": "ts-node-dev --project ./tsconfig.json --respawn --no-deps --rs --cls ./src",
    "build": "tsc",
    "test:unit": "mocha --exit -r ts-node/register src/test/*.ts",
    "test:coverage": "nyc --reporter cobertura --reporter=html --reporter=text mocha -r ts-node/register --require source-map-support/register --recursive src/test/*.ts --reporter mocha-junit-reporter"
  },
  "keywords": [
    "template",
    "typescript",
    "docker"
  ],
  "author": "Florian Vancura",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/chai-http": "^4.2.0",
    "@types/express": "^4.17.14",
    "@types/js-yaml": "^4.0.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.6",
    "@types/request": "^2.48.8",
    "@types/ws": "^8.5.3",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.2.0",
    "mocha-junit-reporter": "^2.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@types/node-cron": "^3.0.6",
    "@types/swagger-ui-express": "^4.1.3",
    "axios": "^1.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "js-yaml": "^4.1.0",
    "mongoose": "^6.8.0",
    "node-cron": "^3.0.2",
    "request": "^2.88.2",
    "swagger-ui-express": "^4.6.1",
    "ws": "^8.11.0"
  }
}
